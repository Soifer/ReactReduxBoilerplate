{
  "name": "mylocations",
  "version": "1.0.0",
  "description": "location diary",
  "main": "index.js",
  "scripts": {
    "test": "mocha  --compilers js:babel-core/register --reporter nyan  src/tools/testSetup.js \"src/**/*.test.js\" ",
    "test:watch": "npm run test -- --watch",
    "babel": "babel",
    "webpack:prod": "SET NODE_ENV=production && webpack -p --devtool cheap-module-source-map",
    "webpack": "webpack",
    "serve": "webpack-dev-server --progress --hot --inline --history-api-fallback",
    "eslint": "lint",
    "start": "npm-run-all --parallel  test:watch serve "
  },
  "compilerOptions": {
    "target": "ES6"
  },
  "author": "Mark Soifer",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "bootstrap": "^3.3.7",
    "bootstrap-css": "^4.0.0-alpha.5",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "esformatter": "^0.10.0",
    "esformatter-jsx": "^7.4.1",
    "eslint": "^3.19.0",
    "eslint-config-eslint": "^3.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.20.2",
    "file-loader": "^0.10.0",
    "jquery": "^3.2.1",
    "jsdom": "10.1.0",
    "jsdom-global": "3.0.2",
    "lint": "^1.1.2",
    "material-ui": "^0.18.3",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "whatwg-url": "^4.7.1"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "quotes": {
      "type": "single"
    },
    "jsx": {
      "formatJSXExpressions": true,
      "JSXExpressionsSingleLine": true,
      "formatJSX": true,
      "attrsOnSameLineAsTag": true,
      "maxAttrsOnTag": 1,
      "firstAttributeOnSameLine": false,
      "spaceInJSXExpressionContainers": " ",
      "alignWithFirstAttribute": true,
      "htmlOptions": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "preserve_newlines": true
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "babel-preset-stage-1": "^6.24.1",
    "eslint-config-defaults": "^9.0.0",
    "google-map-react": "^0.24.0",
    "jsx-ast-utils": "^1.4.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.18.3",
    "material-ui-icons": "^1.0.0-alpha.19",
    "prop-types": "^15.5.10",
    "react-router-redux": "^4.0.8",
    "react-swipeable-views": "^0.12.4",
    "react-toastr": "^2.8.2",
    "toastr": "^2.1.2"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "react"
  ]
}
